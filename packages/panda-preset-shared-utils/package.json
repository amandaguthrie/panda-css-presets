{
  "name": "@amandaguthrie/panda-preset-shared-utils",
  "version": "0.2.0",
  "description": "Shared utilities for other Panda presets.",
  "author": {
    "name": "Amanda Guthrie",
    "url": "https://github.com/amandaguthrie"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amandaguthrie/panda-css-presets.git",
    "directory": "packages/panda-preset-shared-utils"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "source": "./src/index.ts",
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "side-effects": false,
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup --entry src/index.ts --entry src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch",
    "test": "node --import tsx --test test/test.mts"
  },
  "dependencies": {
    "deepmerge-ts": "^5.1.0"
  },
  "devDependencies": {
    "@amandaguthrie/panda-preset-dev-utils": "workspace:*",
    "@pandacss/dev": "<1.0.0",
    "@pandacss/types": "<1.0.0"
  },
  "peerDependencies": {
    "@pandacss/dev": "0.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
