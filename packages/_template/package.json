{
	"name": "package-template",
	"private": "true",
	"version": "0.0.0",
	"description": "A template for packages",
	"author": {
		"name": "Amanda Guthrie",
		"url": "https://github.com/amandaguthrie"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/amandaguthrie/panda-css-presets.git",
		"directory": "packages/_template"
	},
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"required": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"side-effects": false,
	"scripts": {
		"build": "tsdown",
		"build-fast": "tsdown --no-dts",
		"dev": "pnpm build-fast --watch",
		"test": "node --import tsx test/test.mts"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"devDependencies": {
		"tsdown": "catalog:",
		"tsx": "catalog:",
		"typescript": "catalog:"
	}
}
