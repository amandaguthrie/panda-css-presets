{
	"name": "@amandaguthrie/panda-preset-framework-simplecss",
	"version": "0.2.1",
	"description": "A Panda CSS preset based on simple.css",
	"author": {
		"name": "Amanda Guthrie",
		"url": "https://github.com/amandaguthrie"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/amandaguthrie/panda-css-presets.git",
		"directory": "packages/panda-preset-framework-simplecss"
	},
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"required": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"side-effects": false,
	"scripts": {
		"build": "tsdown",
		"build-fast": "tsdown --no-dts",
		"dev": "pnpm build-fast --watch",
		"test": "node --import tsx test/test.mts"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@amandaguthrie/panda-preset-shared-utils": "workspace:*"
	},
	"devDependencies": {
		"@pandacss/dev": "catalog:dev",
		"tsdown": "catalog:",
		"tsx": "catalog:",
		"typescript": "catalog:"
	},
	"peerDependencies": {
		"@pandacss/dev": ">=0.20.1",
		"@pandacss/preset-base": ">=0.20.1"
	}
}
