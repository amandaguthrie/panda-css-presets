{
	"name": "@amandaguthrie/panda-preset-dev-utils",
	"version": "0.2.0",
	"description": "Shared dev utilities for Panda presets and plugins.",
	"author": {
		"name": "Amanda Guthrie",
		"url": "https://github.com/amandaguthrie"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/amandaguthrie/panda-css-presets.git",
		"directory": "packages/panda-preset-dev-utils"
	},
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"required": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"side-effects": false,
	"scripts": {
		"build": "tsdown",
		"build-fast": "tsdown --no-dts",
		"dev": "pnpm build-fast --watch"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=20"
	},
	"devDependencies": {
		"tsdown": "catalog:",
		"typescript": "catalog:"
	}
}
